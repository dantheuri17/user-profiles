{{> messages }}
{{#if username}}
<h2>{{ username }}'s profile</h2>
<table class="table">
  <tr>
    <td>university:</td>
    <td>{{ university }}</td>
  </tr>
  <tr>
    <td>email:</td>
    <td>{{ email }}</td>
  </tr>
  <tr>
    <td>location:</td>
    <td>{{ location }}</td>
  </tr>
  <tr>
    <td>Facebook:</td>
    <td>{{ facebook }}</td>
  </tr>
  <tr>
    <td>Transcripts</td>
    <td>{{ transcriptse }}</td>
  </tr>
</table>
{{/if}}